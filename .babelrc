{
    "presets": [["@babel/preset-env",
        {
          "targets": {
            "node": "current"
           }
        }], ["@babel/preset-react",
          {
        "runtime": "automatic"
      }]
    ],
    "plugins": [["@babel/plugin-transform-runtime"],
        "@babel/plugin-proposal-export-default-from",
         ["@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ]
    ]
}
